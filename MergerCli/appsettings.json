{
  "GENERAL": {
    "validate": false,
    "resumeOutputFolder": "./",
    "uploadOnly": false,
    "backup": true,
    "parallel": {
      "numOfThreads": 1
    }
  },
  "GPKG": {
    "vacuum": false
  },
  "S3": {
    "url": "http://localhost:9000",
    "bucket": "tiles",
    "request": {
      "timeoutSec": 5,
      "retries": 3
    },
    "logToConsole": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient.httpClient.ClientHandler": "Warning",
      "System.Net.Http.HttpClient.httpClient.LogicalHandler": "Warning"
    }
  },
  "TRACING": {
    "enabled": false,
    "url": "",
    "ratio": 1
  },
  "METRICS": {
    "enabled": false,
    "url": "",
    "interval": 3000
  },
  "HTTP": {
    "retries": 3
  }
}
